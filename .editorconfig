# http://editorconfig.org

root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.ts]
quote_type = single
spaces_around_brackets = inside

[*.html]
quote_type = double

[*.md]
trim_trailing_whitespace = false
